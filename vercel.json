{
  "version": 2,
  "name": "protv-freelance-jobs",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "build": {
    "env": {
      "REACT_APP_BACKEND_URL": "https://protv-freelance-jobs.vercel.app"
    }
  }
}
